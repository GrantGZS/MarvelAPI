
<h1>Show Repos</h1>

<table class="table table-bordered table-striped">
    <thead>
        <th>name</th>
    </thead>
    <tbody>
        <% for(repo of repos){%> 
        <tr> 
            <td><%= repo.full_name %> </td> 
            <td><a href><%= repo.html_url %></a></td>
        </tr>
        <% } %>
    </tbody>
</table>

<pre><%= JSON.stringify(repos[0],null,5) %> </pre>